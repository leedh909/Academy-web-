$(function(){
	getJson();
});

//대소문자 조심해라
function getJson(){
	
	$.getJSON("json/bike.json",function(data){
		$.each(data,function(key,val){
			if(key=="DESCRIPTION"){
				$("table").attr("border","1");
				$("thead").append(
						"<tr>"+
						"<th>"+val.ADDR_GU+"</th>"+
						"<th>"+val.CONTENT_ID+"</th>"+
						"<th>"+val.CONTENT_NM+"</th>"+
						"<th>"+val.NEW_ADDR+"</th>"+
						"<th>"+val.CRADLE_COUNT+"</th>"+
						"<th>"+val.LONGITUDE+"</th>"+
						"<th>"+val.LATITUDE+"</th>"+
						"</tr>"
				);
			}else{
				var list = val;
				for(var i=0; i<list.length;i++){
					
					var str = list[i];
					$("tbody").append(
							"<tr>"+
							"<td>"+str.addr_gu+"</td>"+
							"<td>"+str.content_id+"</td>"+
							"<td>"+str.content_nm+"</td>"+
							"<td>"+str.new_addr+"</td>"+
							"<td>"+str.cradle_count+"</td>"+
							"<th>"+str.longitude+"</th>"+
							"<td>"+str.latitude+"</td>"+
							"<input type='hidden' name='bike' value=''>"+
							"</tr>"
							
					);
					
					
				}
				
				
				
			}
		});
		
		
	});
	
	
	
}