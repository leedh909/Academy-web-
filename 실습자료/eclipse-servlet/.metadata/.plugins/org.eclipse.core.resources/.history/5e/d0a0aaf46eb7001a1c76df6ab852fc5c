public class MyBoardDao {

	Connection con = null;

	public MyBoardDao() {

		
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
			System.out.println("디비 연결");
		} catch (ClassNotFoundException e) {
			System.out.println("디비 연결 실패");
			e.printStackTrace();
		}
		
	
	}
	
	
	
	
}
