	public static void Connection() {
		
		try {
			Class.forName("oracle.jdbc.driver.OracleDriver");
			System.out.println("01. 드라이버 연결");
			
		} catch (ClassNotFoundException e) {
			System.out.println("01. 드라이버 연결 오류");
			e.printStackTrace();
		}
		
		
	}
	
	
	
	
	
	
	
