package com.test02;

public class Emp {

	private String name;
	private String money;
	
	public Emp() {
	}
	
	public Emp(String name, String money) {
		super();
		this.name = name;
		this.money = money;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getMoney() {
		return money;
	}
	public void setMoney(String money) {
		this.money = money;
	}

	@Override
	public String toString() {
		return "이름: " + name + ", 돈: " + money;
	}
	
	
	
	
	
}
