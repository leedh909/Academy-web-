package com.test01;

public class MyClass {

	public MyClass() {
		System.out.println("MyClass constructor call!");
	}
	public void prn() {
		System.out.println("prn() 메서드 호출!");
	}
	
	public void setMyClass(MyClass myClass) {
		this.myClass=myClass;
		System.out.println("setMyClass() 호출: "+myClass);
	}
}
